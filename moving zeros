def move_zeros_two_lists(nums):
    non_zeros = []   /// 2 lists 1 for non zer and other for zero and in return add both so we will get final funtion 
    zeros = []
    
    # Logic: Separate the elements
    for x in nums:
        if x == 0:
            zeros.append(x)
        else:
            non_zeros.append(x)
            
    # Logic: Combine them back (The + operator joins lists)
    return non_zeros + zeros

# Test
print(move_zeros_two_lists([0, 1, 0, 3, 12])) # Output: [1, 3, 12, 0, 0]
